{
	"name" : "Http",
	"globIgnorePaths": ["**/*.server.lua", "**/*.client.lua", "**/*.spec.lua"],
	"tree" : {
		"$className" : "ModuleScript",
		"$properties" : {
			"Source" : "return require(script.Src)"
		},

		"Src" : {
			"$path" : "./Src"
		},

		"Packages" : {
			"$path" : "./Packages"
		}
	}
}